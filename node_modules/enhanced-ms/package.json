{
  "name": "enhanced-ms",
  "version": "2.2.0",
  "license": "MIT",
  "description": "Convert human readable time-frame strings to milliseconds and vice versa, with support for i10n and operators!",
  "homepage": "https://github.com/apteryxxyz/enhanced-ms#readme",
  "author": {
    "name": "ApteryxXYZ",
    "url": "https://apteryx.xyz"
  },
  "main": "./dist/index.js",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/apteryxxyz/enhanced-ms"
  },
  "bugs": {
    "url": "https://github.com/apteryxxyz/enhanced-ms/issues"
  },
  "keywords": [
    "convert",
    "duration",
    "human",
    "humanise",
    "humanize",
    "microseconds",
    "milliseconds",
    "ms",
    "number",
    "parse",
    "period",
    "pretty",
    "print",
    "range",
    "readable",
    "seconds",
    "string",
    "time"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/babel__core": "^7.1.20",
    "@types/babel__preset-env": "^7.9.2",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "babel-loader": "^8.3.0",
    "bundle-declarations-webpack-plugin": "^3.1.1",
    "eslint": "^8.28.0",
    "eslint-config-apteryx": "^2.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sonarjs": "^0.16.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^44.0.2",
    "gen-esm-wrapper": "^1.1.3",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-config-apteryx": "^2.0.2",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "ts-config-apteryx": "^2.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "build",
          "chore",
          "ci",
          "docs",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "style",
          "test",
          "types"
        ]
      ]
    }
  },
  "prettier": "prettier-config-apteryx",
  "eslintConfig": {
    "extends": [
      "apteryx/common",
      "apteryx/typescript",
      "apteryx/prettier"
    ],
    "ignorePatterns": [
      "test/*",
      "examples/*",
      "scripts/*",
      "dist/*",
      "temp/*",
      "_src/*",
      "*.config.js"
    ],
    "rules": {
      "@typescript-eslint/prefer-regexp-exec": "off",
      "@typescript-eslint/no-implied-eval": "off",
      "no-new-func": "off",
      "id-length": [
        "error",
        {
          "min": 2,
          "exceptions": [
            "c",
            "a",
            "b",
            "i"
          ]
        }
      ]
    }
  },
  "lint-staged": {
    "src/**/*.ts": "eslint src --fix"
  },
  "scripts": {
    "lint": "eslint src && prettier src --check",
    "format": "eslint src --fix && prettier src --write",
    "test": "jest",
    "build:compile": "tsc && webpack && rimraf temp",
    "build:module": "gen-esm-wrapper . ./dist/index.mjs",
    "build": "pnpm build:compile && pnpm build:module"
  }
}